<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<?import java.net.URL?>
<Pane fx:controller="com.comp2042.ui.GuiController"
      xmlns:fx="http://javafx.com/fxml">
    <GridPane fx:id="brickPanel" vgap="1" hgap="1"/>
    <BorderPane styleClass="gameBoard" fx:id="gameBoard">
        <center>
            <GridPane fx:id="gamePanel" hgap="1" vgap="1"/>
        </center>
    </BorderPane>

    <Group fx:id="comboNotificationGroup" layoutX="200" layoutY="100">
    </Group>
    <Group fx:id="scoreNotificationGroup" layoutX="200" layoutY="100">
    </Group>
    <Group fx:id="centerNotificationGroup" layoutX="200" layoutY="100">
    </Group>
    <stylesheets>
        <URL value="@window_style.css"/>
    </stylesheets>
</Pane>